using System;

class Program
{
    static void Main()
    {
        string str = "Hello World";
        int vowelCount = 0;

        foreach (char c in str)
        {
            if ("aeiouAEIOU".Contains(c))
            {
                vowelCount++;
            }
        }

        Console.WriteLine("Number of vowels: " + vowelCount);
    }
}
