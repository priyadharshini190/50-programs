using System;
public class SquareNumber
{
    public static void Main()
    {
        Console.WriteLine("Enter a Number : ");
        int Number = Convert.ToInt16(Console.ReadLine());
        double Sqr = Math.Pow(Number, 2);
        Console.WriteLine("Square of {0} is: {1}", Number, Sqr);
        Console.ReadLine();
    }
}
